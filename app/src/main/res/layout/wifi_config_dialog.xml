<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="882px"
        android:layout_height="540px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/dock_set_bnt_password_background" />

    <EditText
        android:id="@+id/wifi_password"
        android:layout_width="260px"
        android:layout_height="wrap_content"
        android:layout_marginTop="250px"
        android:layout_marginStart="330px"
        android:textSize="32px"
        android:background="@null"
        android:hint="请输入密码"
        android:inputType="textNoSuggestions"
        android:textColor="#3399fe"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <ImageView
        android:id="@+id/wifi_dialog_cancel"
        android:layout_width="162px"
        android:layout_height="72px"
        android:layout_marginEnd="200px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/wifi_dialog_confirm"
        app:srcCompat="@mipmap/scrap_page_bnt_cancel" />

    <ImageView
        android:id="@+id/wifi_dialog_confirm"
        android:layout_width="162px"
        android:layout_height="72px"
        android:layout_marginStart="200px"
        android:layout_marginBottom="50px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/dock_set_bnt_password_ok" />
</androidx.constraintlayout.widget.ConstraintLayout>