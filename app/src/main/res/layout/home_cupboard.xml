<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="146px"
    android:layout_height="242px">


    <ImageView
        android:id="@+id/home_cupboard_state"
        android:layout_width="90px"
        android:layout_height="142px"
        android:layout_marginTop="30px"
        android:layout_marginEnd="28px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/home_cupboard_state" />

    <TextView
        android:id="@+id/home_cupboard_name"
        android:layout_width="90px"
        android:layout_height="30px"
        android:layout_marginTop="10px"
        android:background="@color/black"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="19px"
        app:layout_constraintEnd_toEndOf="@+id/home_cupboard_state"
        app:layout_constraintTop_toBottomOf="@+id/home_cupboard_state" />

</androidx.constraintlayout.widget.ConstraintLayout>